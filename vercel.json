{
    "version": 2,
    "builds": [
      {
        "src": "Client/package.json",
        "use": "@vercel/node",
        "config": {
          "distDir": "Client/dist"
        }
      },
      {
        "src": "Server/package.json",
        "use": "@vercel/node",
        "config": {
          "distDir": "Server/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  