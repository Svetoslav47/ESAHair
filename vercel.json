{
    "version": 2,
    "builds": [
      {
        "src": "Client/package.json",
        "use": "@vercel/node",
        "config": {
          "distDir": "Client/dist"
        }
      },
      {
        "src": "Server/index.js",
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/Server/index.js"
      }
    ]
  }
  